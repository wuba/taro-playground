    "update-version": "sh ./bin/update_version.sh",
    "podInstall": "pod-install",
    "upgradePeerdeps": "install-peerdeps @tarojs/taro-rn@4.0.0-canary.9 -o -P && install-peerdeps @tarojs/components-rn@4.0.0-canary.9 -o -P && install-peerdeps @tarojs/router-rn@4.0.0-canary.9 -o -P && pod-install"
